{
  "manifest_version": 2,
  "name": "Hide Itch Analytics",
  "version": "1.0",
  "description": "Hide analytics on itch dashboard.",
  "permissions": [
    "https://itch.io/dashboard"
  ],
  "content_scripts": [
    {
      "matches": ["*://itch.io/dashboard"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
